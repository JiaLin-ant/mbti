{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/sitemap.xml",
      "headers": {
        "Content-Type": "application/xml"
      },
      "continue": true
    },
    {
      "src": "/robots.txt",
      "headers": {
        "Content-Type": "text/plain"
      },
      "continue": true
    }
  ]
} 