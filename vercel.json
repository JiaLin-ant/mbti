{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain"
        }
      ]
    }
  ],
  "redirects": [
    { 
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/chinese/index.html",
      "destination": "/chinese/",
      "permanent": true
    },
    {
      "source": "/love-match/en/index.html",
      "destination": "/love-match/en/",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/love-match", "destination": "/love-match/index.html" },
    { "source": "/love-match/en", "destination": "/love-match/en/index.html" },
    { "source": "/chinese", "destination": "/chinese/index.html" }
  ]
}